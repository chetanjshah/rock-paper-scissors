pipeline {
  agent any 
  stages {
    stage('Log Jenkins Maven Docker Git and Java version info') {
      steps {
        sh 'mvn -version'
		sh 'java -version'
		sh 'git --version'
      }
    }
    
    stage('GitHub Jenkins Maven Docker Build') {
      steps {
        sh 'mvn compile test install'
      }
    }
    
    stage('Build with Jenkins Maven plugin') {
      steps {
	withMaven(maven: 'maven-latest') {
          sh 'mvn compile test install'
        }
      }
    }
  }
}
